{
	"name": "turret",
	"version": "2.0.3",
	"author": "Scott de Jonge",
	"description": "A responsive frontend framework build with LESS to normalize styles for default HTML elements",
	"repository": {
		"type": "git",
		"url": "git@code.bigfish.tv:turret.git"
	},
	"dependencies": {
		"browserify": "latest",
		"uglify-js": "latest",
		"less": "2.2.0"
	},
	"devDependencies": {
		"watchify": "latest",
		"catw": "latest"
	},
	"scripts": {
		"watch": "catw -c 'lessc -' 'docs/styles/styles.less' -o docs/css/styles.css -v",
		"build": "lessc -x docs/styles/styles.less > docs/css/styles.css"
	}
}